class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
            self.head = None

    def print_LinkedList(self):
        temp = self.head
        while (temp):
            print(temp.data),
            temp = temp.next


#driver method
if __name__ == "__main__":

    linkedLst = LinkedList()

    linkedLst.head = Node(1)
    second= Node(2)
    third = Node(3)

    linkedLst.head.next = second
    second.next = third

    linkedLst.print_LinkedList()



