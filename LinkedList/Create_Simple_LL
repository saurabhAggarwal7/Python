class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
            self.head = None

#driver method
if __name__ == "__main__":

    linkedLst = LinkedList()

    linkedLst.head = Node(1)
    second= Node(2)
    third = Node(3)

    linkedLst.head.next = second
    second.next = third



